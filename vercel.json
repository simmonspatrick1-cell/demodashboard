{
  "version": 2,
  "builds": [
    {
      "src": "backend-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend-server.js"
    }
  ],
  "env": {
    "NETSUITE_ACCOUNT_ID": "@netsuite-account-id",
    "ANTHROPIC_API_KEY": "@anthropic-api-key"
  }
}