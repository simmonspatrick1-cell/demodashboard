{
  "framework": null,
  "buildCommand": "npm install --include=dev && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://demodashboard-production.up.railway.app/api/:path*"
    }
  ],
  "env": {
    "VITE_API_URL": "https://demodashboard-production.up.railway.app"
  }
}
